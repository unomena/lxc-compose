# SearXNG Environment Variables

# Secret key for SearXNG (generate with: openssl rand -hex 16)
# If not set, will be auto-generated during installation
SEARXNG_SECRET=changeme_use_openssl_rand_hex_16

# SearXNG instance name
SEARXNG_INSTANCE_NAME=SearXNG

# Contact URL for your instance
SEARXNG_CONTACT_URL=

# Enable image proxy (recommended for privacy)
SEARXNG_IMAGE_PROXY=true

# Enable request limiter
SEARXNG_LIMITER=true

# Redis connection for caching
REDIS_HOST=127.0.0.1
REDIS_PORT=6379

# Debug mode (set to False in production)
DEBUG=False